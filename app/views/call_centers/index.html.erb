<p id="notice"><%= notice %></p>

<div class="container">
  <div class="page-header">
    <h2> جميع المنشأت </h2>
  </div>
</div>

<div class="container">
<table class="table table-striped">
  <thead>
    <tr>
      <th> Name </th>
      <th> address </th>
      <th> phone_numer </th>
      <th> services </th>
      <th> email </th>
      <th> description </th>
      <th> status </th>
      <th> created_at </th>
      <th> Actions </th>
    </tr>
  </thead>

  <tbody>
    <% @call_centers.each do |call_center| %>
      <tr>
        <td><%= call_center.name %></td>
        <td><%= call_center.address %></td>
        <td><%= call_center.phone_numer %></td>
        <td><%= call_center.services %></td>
        <td><%= call_center.email %></td>
        <td><%= call_center.description %></td>
        <td><%= call_center.status %></td>

        <td><%= call_center.created_at %></td>
        <td><%= link_to 'Show', call_center %>  <%= link_to 'Edit', edit_call_center_path(call_center) %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>
</div>