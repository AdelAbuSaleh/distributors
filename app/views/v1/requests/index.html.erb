<p id="notice"><%= notice %></p>

<h1>V1 Requests</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @v1_requests.each do |v1_request| %>
      <tr>
        <td><%= link_to 'Show', v1_request %></td>
        <td><%= link_to 'Edit', edit_v1_request_path(v1_request) %></td>
        <td><%= link_to 'Destroy', v1_request, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New V1 Request', new_v1_request_path %>
