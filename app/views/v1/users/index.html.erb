<p id="notice"><%= notice %></p>

<h1>V1 Users</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @v1_users.each do |v1_user| %>
      <tr>
        <td><%= link_to 'Show', v1_user %></td>
        <td><%= link_to 'Edit', edit_v1_user_path(v1_user) %></td>
        <td><%= link_to 'Destroy', v1_user, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New V1 User', new_v1_user_path %>
